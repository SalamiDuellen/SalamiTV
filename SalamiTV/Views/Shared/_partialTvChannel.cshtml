@model SalamiTV.ViewModels.TemporaryViewModel

<section class="container-fluid">
    <div id="tvchannel">
        <div class="row">
            @foreach (var channel in Model.TvChannels)
            {
                <div class="col-xs-12 col-sm-6 col-md-3">
                    <h2 id="channelheader">
                        @Html.Display(channel.Name)
                    </h2>
                    @Html.Action("Partial2", "Home", new { id = channel.ID, page = Model.Page })
                </div>
            }
        </div>
    </div>
</section>